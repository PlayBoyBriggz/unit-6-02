import Foundation

enum DaysOfTheWeek : String {
    // this  holds the days of the week
    
    case Monday
    case Tuesday
    case Wedensday
    case Thursday
    case Friday
    case Saturday 
    case Sunday 
    
    var code : Int {
        switch self {
        case .Monday: return 1 
        case .Tuesday: return 2 
        case .Wedensday: return 3 
        case .Thursday: return 4 
        case .Friday: return 5
        case .Saturday: return 6
        case .Sunday: return 7
        }
    }
}

var favouriteDay : DaysOfTheWeek

show("Enter your favoirite day of the week: ")
let input = ask()
favouriteDay = DaysOfTheWeek(rawValue: input)!
show("That is day \(favouriteDay.code)") 



